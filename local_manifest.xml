<?xml version="1.0" encoding="UTF-8"?>

<manifest>

        <remote name="arata-labs"
        sync-c="true"
        sync-j="4"
        fetch="https://gitlab.com/HinohArata" />

        <remote name="arata-hub"
        sync-c="true"
        sync-j="4"
        fetch="https://github.com/Arata-Stuffs" />

        <remote name="hub"
        sync-c="true"
        sync-j="4"
        fetch="https://github.com" />

        <remote name="ros"
        sync-c="true"
        sync-j="4"
        fetch="https://github.com/RisingTechOSS-devices"
        revision="fifteen" />

        <!--
        <remote name="gitlabs"
        sync-c="true"
        sync-j="4"
        fetch="https://gitlab.com" />
        -->

        <!-- Sync trees -->
        <project path="device/xiaomi/surya" name="device_xiaomi_surya" remote="ros" />
        <project path="vendor/xiaomi/surya" name="vendor_xiaomi_surya" remote="ros" />
        <project path="kernel/xiaomi/surya" name="kernel_xiaomi_surya" remote="ros" />
        <project path="device/xiaomi/surya/parts" name="temporary-xp" remote="arata-labs" revision="xp" />
        <project path="firmware/xiaomi/surya" name="firmware_xiaomi_surya" remote="arata-labs" revision="new" />

        <!-- MiuiCamera -->
        <project path="device/xiaomi/surya-miuicamera" name="temporary-d1" remote="arata-labs" revision="camd" />
        <project path="vendor/xiaomi/surya-miuicamera" name="temporary-v1" remote="arata-labs" revision="camv" />

        <!-- Keys -->
        <project path="vendor/private-keys/keys" name="temporary-k" remote="arata-labs" revision="lock" />

        <!-- additional -->
        <project path="vendor/prebuilts/GalleryGO" name="vendor_prebuilts_GalleryGO" remote="arata-hub" revision="main" />

</manifest>
